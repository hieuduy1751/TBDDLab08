<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTintMode="add"
    tools:context=".welcome">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="53dp"
        android:layout_height="55dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/group_3" />

    <Button
        android:id="@+id/btnSI"
        android:layout_width="276dp"
        android:layout_height="66dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="28dp"
        android:backgroundTint="#EF77B0"
        android:paddingStart="80dp"
        android:paddingTop="17dp"
        android:paddingEnd="80dp"
        android:paddingBottom="17dp"
        android:text="REGISTER"
        android:textSize="18sp"
        app:cornerRadius="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtPasswordLG" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="413dp"
        android:layout_height="109dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/intersect__1_"
        tools:layout_editor_absoluteY="631dp" />

    <EditText
        android:id="@+id/txtPasswordRG"
        android:layout_width="274dp"
        android:layout_height="65dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="288dp"
        android:ems="10"
        android:hint="Type Password"
        android:inputType="textPassword"
        android:textColorHint="#B0000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtEmailRG"
        android:layout_width="274dp"
        android:layout_height="65dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="208dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:textColorHint="#B0000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtNameRG"
        android:layout_width="274dp"
        android:layout_height="65dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="128dp"
        android:ems="10"
        android:hint="Your name"
        android:inputType="text"
        android:textColorHint="#B0000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtPasswordLG"
        android:layout_width="274dp"
        android:layout_height="62dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:hint="Type Password"
        android:inputType="textPassword"
        android:textColorHint="#B0000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtPasswordRG" />

    <Button
        android:id="@+id/btnGGRG"
        android:layout_width="253dp"
        android:layout_height="38dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="44dp"
        android:background="@android:color/transparent"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:text="REGISTER WITH GOOGLE"
        android:textColor="#B02D58F3"
        android:textSize="16dp"
        app:iconPadding="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSI" />

    <Button
        android:id="@+id/btnSIRG"
        android:layout_width="153dp"
        android:layout_height="50dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="45dp"
        android:background="@android:color/transparent"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:text="Sign in"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="16dp"
        android:textStyle="bold"
        app:iconPadding="0dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSI"
        app:srcCompat="@drawable/grommet_icons_google" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="56dp"
        android:text="Already have an account?"
        android:textColor="#CCFFFFFF"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginTop="52dp"
        android:text="CREATE NEW ACCOUNT"
        android:textColor="#B0000000"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>